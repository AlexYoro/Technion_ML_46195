// Tree
digraph {
	root [label=<<I><B>x</B></I>> shape=plaintext]
	node_1 [label=<x<SUB>3</SUB>> color="#6C8EBF" fillcolor="#DAE8FC" penwidth=3 style=filled]
	root -> node_1 [penwidth=2]
	leaf_1_1 [label=1 shape=plaintext]
	node_1 -> leaf_1_1 [label=-1 penwidth=2]
	leaf_1_2 [label=-1 shape=plaintext]
	node_1 -> leaf_1_2 [label=1 penwidth=2]
}
